<script>
<name>plugin.jj.colour.by.race.get.races</name>
<version>1</version>
<engineversion>62</engineversion>
<description></description>
<arguments>
<argument index="1" name="criminals" type="Var/Boolean" desc="criminals" />
<argument index="2" name="disguises" type="Var/Boolean" desc="disguises" />
</arguments>
<sourceplaintext>
<![CDATA[
  * ************************************************
  * SCRIPT NAME: Get Races
  * DESCRIPTION: Separate script to get races. We'll have a version for AP and a different version for FL
  * 
  * AUTHOR: JJ     DATE: 31 March 2023
  * ************************************************
  $engine = script engine version
  if $criminals == [TRUE]
    $races = array alloc: size=2
    $races[0] = Pirates
    $races[1] = Yaki
    if $engine >= 61
      resize array $races to 3
      $races[2] = Duke's
    end
    do if $disguises != [TRUE]
      return $races
  end
  if $disguises == [TRUE]
    $disguises = array alloc: size=5
    $disguises[0] = Argon
    $disguises[1] = Split
    $disguises[2] = Paranid
    $disguises[3] = Boron
    $disguises[4] = Teladi
    do if $criminals != [TRUE]
      return $disguises
    $XS.Iterator1 = size of array $races
    while $XS.Iterator1
      dec $XS.Iterator1 = 
      $race = $races[$XS.Iterator1]
      append $race to array $disguises
    end
    return $disguises
  end
  $races = array alloc: size=11
  $races[0] = Argon
  $races[1] = Split
  $races[2] = Paranid
  $races[3] = Terran
  $races[4] = Race 2
  $races[5] = Pirates
  $races[6] = Yaki
  $races[7] = Goner
  $races[8] = ATF
  $races[9] = Boron
  $races[10] = Teladi
  if $engine >= 61
    $flRaces = array alloc: size=9
    $flRaces[0] = OTAS
    $flRaces[1] = TerraCorp
    $flRaces[2] = Atreus
    $flRaces[3] = NMMC
    $flRaces[4] = Strong Arms
    $flRaces[5] = Beryll
    $flRaces[6] = Duke's
    $flRaces[7] = Darkspace
    $flRaces[8] = Industritech
    $XS.Iterator1 = size of array $flRaces
    while $XS.Iterator1
      dec $XS.Iterator1 = 
      $race = $flRaces[$XS.Iterator1]
      append $race to array $races
    end
  end
  return $races
]]>
</sourceplaintext>
